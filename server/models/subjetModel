const Sequelize = require('sequelize');
const db = require('../database/db.connect');

const subject = db.define('subject', {
    id_subject: {
        type: Sequelize.INTEGER,
        allowNull: false,
        unsigned: true,
        autoIncrement: true,
        unique: true,
        primaryKey: true
    },
    name: {
        type: Sequelize.STRING,
    }
});

module.exports = subject;